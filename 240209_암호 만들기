#include <iostream>
#include <string>
#include <map>
#include <vector>
#include <algorithm>
#include <cmath>

using namespace std;

void combinations(vector<int> current, int l, int start, int c , vector<vector<int>>& result){
    if(l==0){
        result.push_back(current);
    }
    else{
        if (l<=c-start){
            current.push_back(start);
            combinations(current, l-1, start+1, c, result);
            current.pop_back();
            combinations(current, l, start+1, c, result);
        }
    }
}

int main(){
    int l,c;
    cin>>l>>c;
    char arr[15];
    for (int i=0; i<c; i++){
        cin>>arr[i];
    }
    sort(arr,arr+c);
    vector<int> current;
    vector<vector<int>> result;
    combinations(current, l, 0, c, result);
    for (int i=0; i<result.size(); i++){
        int m=0;
        int g=0;
        for (int j=0; j<result[i].size(); j++){
            int ans=arr[result[i][j]];
            if (ans=='a' | ans=='e' | ans=='i' | ans=='o' | ans=='u') m+=1;
            else g+=1;
        }
        if (m!=0 & g!=0 & g!=1){
            for(int k=0; k<result[i].size();k++){
                cout<<arr[result[i][k]];
            }
            cout<<endl;
        }
    }
}